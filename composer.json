{
    "name": "appwilio/russianpost-sdk",
    "keywords": ["Russian Post", "Почта России", "delivery"],
    "homepage": "https://github.com/appwilio/russianpost-sdk",
    "description": "Russian Post SDK (pochta.ru)",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Roman Sokharev",
            "email": "greabock17@gmail.com",
            "role": "Creator"
        },
        {
            "name": "JhaoDa",
            "email": "jhaoda@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "jhaoda@gmail.com",
        "issues": "https://github.com/appwilio/russianpost-sdk/issues"
    },
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-soap": "*",
        "guzzlehttp/guzzle": "^6.3",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "illuminate/support": "5.1 - 5.8",
        "monolog/monolog": "^1.25",
        "phpunit/phpunit": "^7.5",
        "symfony/var-dumper": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Appwilio\\RussianPostSDK\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Appwilio\\RussianPostSDK\\LaravelServiceProvider"
            ]
        }
    }
}
